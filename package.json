{
  "name": "pug-discord-bot",
  "version": "0.0.1",
  "description": "A bot for creating pugs for overwatch",
  "main": "app.js",
  "scripts": {
    "start-ts": "ts-node app.ts",
    "start": "babel-node --presets @babel/preset-env,@babel/preset-typescript app.ts --extensions \".ts,.js\" ",
    "test": "mocha --require ts-node/register --watch-extensions ts,tsx \"test/**/*.{ts,tsx}\"",
    "debug": "babel-node --inspect --presets @babel/preset-env,@babel/preset-typescript app.ts --extensions \".ts,.js\" ",
    "web": "babel-node --presets @babel/preset-env,@babel/preset-typescript web.ts --extensions \".ts,.js\""
  },
  "author": "Ivan Fernandes",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-typescript": "^7.1.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "discord.js": "^11.4.2",
    "dotenv": "^6.1.0",
    "erlpack": "^0.1.2",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "mongodb": "^3.1.10",
    "node-fetch": "^2.3.0",
    "node-html-parser": "^1.1.11",
    "passport": "^0.4.0",
    "passport-bnet": "^2.0.0",
    "serve-static": "^1.13.2",
    "uws": "^99.0.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/mocha": "^5.2.5",
    "@types/mongodb": "^3.1.14",
    "@types/node": "^10.12.9",
    "@types/passport": "^0.4.7",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  }
}
